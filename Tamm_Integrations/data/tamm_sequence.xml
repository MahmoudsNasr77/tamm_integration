<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_tamm_tracking" model="ir.sequence">
        <field name="name">Tamm Tracking Sequence</field>
        <field name="code">tamm.tracking</field>
        <field name="prefix">TRK/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_tamm_maintenance" model="ir.sequence">
        <field name="name">Tamm Maintenance Sequence</field>
        <field name="code">tamm.maintenance</field>
        <field name="prefix">MNT/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_tamm_route" model="ir.sequence">
        <field name="name">Tamm Route Sequence</field>
        <field name="code">tamm.route</field>
        <field name="prefix">RTE/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
